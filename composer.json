{
    "name": "frankapi/composer-installer",
    "description": "Composer installer for Frank Api modules and plugins",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Barrento",
            "email": "francisco.barrento@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "symfony/console": "^2.5|^3.0|^4.0|^5.0",
        "composer-plugin-api": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "FrankApi\\Composer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FrankApi\\Composer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "class": "FrankApi\\Composer\\Plugin"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "composer/composer": "^2.0@dev",
        "mockery/mockery": "^1.4@dev"
    }
}
